{% extends 'base.html.twig' %}

{% block title %}Connexion{% endblock %}

{% block body %}
    <div class="container-fluid">
        <div class="row justify-content-center">
            <div class="col-md-6 mt-5">
                <div class="card">
                    <div class="card-header">Connexion</div>
                    <div class="card-body">
                        <p>Connectez-vous avec google en un clic.</p>
                        <div class="row justify-content-center">
                            <div class="col-md-6">
                                <a href="{{ url('connect_google') }}" class="btn btn-primary align-middle">
                                    <img class="w-25 float-start img-fluid"
                                         src="data:image/png;base64,"
                                         alt="google">
                                    <span class="align-middle">Connexion avec google</span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
